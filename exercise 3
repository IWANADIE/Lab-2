def main():
    display_main_menu()
    get_user_input()

def display_main_menu():
    print("Enter some number separated by commas(e.g. 5, 67, 32)")
def get_user_input():
    x = input()
    print("You entered: " + x)
    y = x.split(",")
    print(y)
    mylist = y
    print(mylist)
    z = list(map(float, mylist))
    print(z)

    return z

main()